{
  "version": "1.0.0",
  "name": "development-project",
  "description": "Development-focused PRP configuration with debugging enabled",
  "author": "Development Team",
  "license": "MIT",
  "type": "cli",
  "storage": {
    "dataDir": ".prp",
    "cacheDir": "/tmp/prp-cache",
    "worktreesDir": "/tmp/prp-worktrees",
    "notesDir": ".prp/notes",
    "logsDir": "/tmp/prp-logs",
    "keychainFile": ".prp/keychain.json",
    "persistFile": ".prp/state.json",
    "maxCacheSize": 209715200,
    "retentionPeriod": 604800000
  },
  "agents": [
    {
      "id": "claude-code-anthropic",
      "name": "Claude Code Anthropic",
      "type": "claude-code-anthropic",
      "role": "orchestrator-agent",
      "provider": "anthropic",
      "enabled": true,
      "capabilities": {
        "supportsTools": true,
        "supportsImages": true,
        "supportsSubAgents": true,
        "supportsParallel": false,
        "supportsCodeExecution": false,
        "maxContextLength": 200000,
        "supportedModels": ["claude-3-5-sonnet-20241022"],
        "supportedFileTypes": ["*"],
        "canAccessInternet": true,
        "canAccessFileSystem": true,
        "canExecuteCommands": false
      },
      "limits": {
        "maxTokensPerRequest": 100000,
        "maxRequestsPerHour": 50,
        "maxRequestsPerDay": 500,
        "maxCostPerDay": 50,
        "maxExecutionTime": 600000,
        "maxMemoryUsage": 2048,
        "maxConcurrentTasks": 3,
        "cooldownPeriod": 2000
      },
      "authentication": {
        "type": "api-key",
        "credentials": {
          "apiKey": "${ANTHROPIC_API_KEY:-}"
        },
        "encrypted": true
      },
      "personality": {
        "tone": "friendly",
        "language": "en",
        "responseStyle": "detailed",
        "verbosity": "comprehensive",
        "creativity": 0.8,
        "strictness": 0.4,
        "proactivity": 0.6,
        "customInstructions": "You are a helpful development assistant focused on writing clean, maintainable code with comprehensive testing.",
        "communicationStyle": {
          "useEmojis": true,
          "useFormatting": true,
          "includeCodeBlocks": true,
          "includeExplanations": true
        }
      },
      "tools": [],
      "environment": {
        "workingDirectory": "${WORKING_DIRECTORY:-$(pwd)}",
        "shell": "/bin/bash",
        "envVars": {
          "NODE_ENV": "development",
          "DEBUG": "prp:*"
        },
        "nodeVersion": "20",
        "pythonVersion": "3.11",
        "allowedCommands": ["git", "npm", "node", "yarn", "pnpm", "python", "pip", "docker", "make"],
        "blockedCommands": ["rm -rf /", "sudo rm", "chmod 777", "dd if="],
        "networkAccess": {
          "allowedDomains": [],
          "blockedDomains": [],
          "allowExternalRequests": true
        },
        "fileSystem": {
          "allowedPaths": ["${WORKING_DIRECTORY:-$(pwd)}"],
          "blockedPaths": ["/etc", "/usr/bin", "/bin", "/sbin", "/root"],
          "maxFileSize": 52428800,
          "allowWrite": true,
          "allowDelete": false
        }
      },
      "preferences": {
        "autoSave": true,
        "autoCommit": true,
        "preferAsync": true,
        "useCache": true,
        "debugMode": true,
        "logLevel": "debug",
        "notifications": {
          "enabled": true,
          "types": ["error", "warning", "info"],
          "channels": ["console"]
        },
        "git": {
          "autoStage": true,
          "commitMessageFormat": "feat: {description}",
          "branchNaming": "feature/{name}"
        }
      },
      "metadata": {
        "version": "1.0.0",
        "author": "system",
        "createdAt": "2025-11-05T00:00:00.000Z",
        "lastModified": "2025-11-05T00:00:00.000Z",
        "tags": ["default", "anthropic", "claude", "orchestrator", "development"],
        "category": "ai-assistant",
        "description": "Development-focused Claude agent with enhanced debugging capabilities",
        "documentation": "Optimized for development workflows with detailed explanations and comprehensive testing.",
        "examples": [],
        "dependencies": [],
        "compatibility": {
          "platforms": ["linux", "macos", "windows"],
          "nodeVersions": ["18", "20"],
          "pythonVersions": ["3.9", "3.10", "3.11"]
        }
      }
    }
  ],
  "guidelines": [],
  "signals": {},
  "orchestrator": {},
  "scanner": {},
  "inspector": {},
  "tui": {
    "mode": "cli",
    "activeScreen": "main",
    "followEvents": true,
    "autoRefresh": true,
    "refreshInterval": 2000
  },
  "features": {
    "scanner": true,
    "inspector": true,
    "orchestrator": true,
    "tui": true,
    "mcp": true,
    "worktrees": true
  },
  "limits": {
    "maxConcurrentAgents": 5,
    "maxWorktrees": 20,
    "maxPRPsPerWorktree": 10,
    "tokenAlertThreshold": 0.7,
    "tokenCriticalThreshold": 0.9
  },
  "logging": {
    "level": "debug",
    "enableFileLogging": true,
    "enableTokenTracking": true,
    "enablePerformanceTracking": true,
    "logRetentionDays": 3
  },
  "security": {
    "enablePinProtection": false,
    "encryptSecrets": true,
    "sessionTimeout": 1800
  },
  "settings": {
    "debug": {
      "enabled": true,
      "level": "debug",
      "console": true,
      "file": true,
      "timestamp": true,
      "colors": true,
      "profiling": true
    },
    "quality": {
      "linting": {
        "enabled": true,
        "rules": {
          "no-console": "warn",
          "prefer-const": "error"
        },
        "fixOnSave": true
      },
      "testing": {
        "enabled": true,
        "coverage": 90,
        "frameworks": ["jest"]
      },
      "security": {
        "enabled": true,
        "tools": ["npm-audit", "eslint-security"],
        "rules": {
          "no-eval": "error"
        }
      },
      "performance": {
        "enabled": true,
        "thresholds": {
          "loadTime": 2000,
          "bundleSize": 500000
        }
      }
    },
    "build": {
      "tool": "tsc",
      "optimization": false,
      "minification": false,
      "sourceMap": true,
      "target": ["es2020"],
      "output": {
        "directory": "dist",
        "filename": "index.js",
        "format": ["cjs", "esm"]
      }
    },
    "test": {
      "framework": "jest",
      "coverage": {
        "enabled": true,
        "threshold": 90,
        "reporters": ["text", "lcov", "html"]
      },
      "environment": "node",
      "setupFiles": ["./tests/setup.ts"],
      "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"]
    },
    "ci": {
      "platform": "github",
      "workflows": {
        "build": true,
        "test": true,
        "deploy": false,
        "security": true
      },
      "triggers": {
        "onPush": true,
        "onPR": true,
        "onSchedule": false
      },
      "environment": {
        "NODE_ENV": "test",
        "CI": "true"
      }
    },
    "development": {
      "watch": true,
      "hotReload": true,
      "port": 3000,
      "host": "localhost",
      "proxy": {
        "/api": "http://localhost:8080"
      },
      "server": "webpack-dev-server"
    },
    "packageManager": {
      "manager": "npm",
      "autoInstall": true,
      "scripts": {
        "dev": "prp dev",
        "build": "prp build",
        "test": "prp test",
        "test:watch": "prp test --watch",
        "lint": "prp lint",
        "lint:fix": "prp lint --fix",
        "quality": "prp quality"
      },
      "dependencies": {},
      "devDependencies": {}
    }
  },
  "scripts": {
    "dev": "prp dev",
    "build": "prp build",
    "test": "prp test",
    "test:watch": "prp test --watch",
    "lint": "prp lint",
    "lint:fix": "prp lint --fix",
    "quality": "prp quality",
    "clean": "prp clean",
    "reset": "prp reset"
  }
}