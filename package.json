{
  "name": "@dcversus/prp",
  "version": "0.4.9",
  "description": "Interactive Project Bootstrap CLI - Modern scaffolding tool with AI integration for multiple frameworks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "prp": "dist/cli.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc && chmod +x dist/cli.js",
    "build:watch": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "jest tests/e2e --passWithNoTests --runInBand",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "debug:demo": "tsx examples/debug-screen-demo.ts",
    "debug:test": "tsx src/tui/debug-screen.test.ts",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "build:docs": "webpack --mode production && node scripts/add-analytics.js",
    "dev:docs": "webpack serve --mode development",
    "prepublishOnly": "npm run validate && npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "cli",
    "scaffolding",
    "generator",
    "bootstrap",
    "project-setup",
    "yeoman",
    "cookiecutter",
    "ai",
    "codex",
    "typescript",
    "react",
    "fastapi",
    "nestjs"
  ],
  "author": "dcversus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcversus/prp.git"
  },
  "bugs": {
    "url": "https://github.com/dcversus/prp/issues"
  },
  "homepage": "https://github.com/dcversus/prp#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@types/boxen": "^2.1.0",
    "@types/chokidar": "^1.7.5",
    "@types/figlet": "^1.7.0",
    "@types/jsonschema": "^0.0.5",
    "@types/lru-cache": "^7.10.9",
    "axios": "^1.13.1",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "execa": "^9.4.0",
    "figlet": "^1.9.3",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^12.0.1",
    "jsonschema": "^1.5.0",
    "lru-cache": "^11.2.2",
    "nanoid": "^5.0.7",
    "ora": "^8.1.0",
    "react": "^18.3.1",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc-fb9a90fa48-20240614",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
