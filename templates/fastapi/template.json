{
  "name": "FastAPI",
  "description": "FastAPI Python web framework with automatic API documentation and Docker support",
  "version": "0.1.0",
  "category": "backend",
  "language": "python",
  "author": "PRP Framework",
  "license": "MIT",
  "variables": {
    "PROJECT_NAME": {
      "type": "string",
      "description": "Project name",
      "default": "my-fastapi-app"
    },
    "PROJECT_DESCRIPTION": {
      "type": "string",
      "description": "Project description",
      "default": "FastAPI application built with PRP"
    },
    "VERSION": {
      "type": "string",
      "description": "Initial version",
      "default": "0.1.0"
    },
    "AUTHOR": {
      "type": "string",
      "description": "Author name",
      "default": "Developer"
    },
    "EMAIL": {
      "type": "string",
      "description": "Author email",
      "default": "dev@example.com"
    }
  },
  "files": [
    {
      "source": "app/main.py",
      "target": "app/main.py",
      "type": "template"
    },
    {
      "source": "app/__init__.py",
      "target": "app/__init__.py",
      "type": "static"
    },
    {
      "source": "app/routers/__init__.py",
      "target": "app/routers/__init__.py",
      "type": "static"
    },
    {
      "source": "app/routers/items.py",
      "target": "app/routers/items.py",
      "type": "template"
    },
    {
      "source": "app/routers/users.py",
      "target": "app/routers/users.py",
      "type": "template"
    },
    {
      "source": "requirements.txt",
      "target": "requirements.txt",
      "type": "template"
    },
    {
      "source": ".env.example",
      "target": ".env.example",
      "type": "static"
    },
    {
      "source": ".gitignore",
      "target": ".gitignore",
      "type": "static"
    },
    {
      "source": "README.md",
      "target": "README.md",
      "type": "template"
    },
    {
      "source": "Dockerfile",
      "target": "Dockerfile",
      "type": "template"
    },
    {
      "source": "docker-compose.yml",
      "target": "docker-compose.yml",
      "type": "template"
    },
    {
      "source": ".github/workflows/ci.yml",
      "target": ".github/workflows/ci.yml",
      "type": "template"
    }
  ],
  "postCreate": [
    "python -m venv venv",
    "source venv/bin/activate || venv\\Scripts\\activate",
    "pip install -r requirements.txt",
    "cp .env.example .env",
    "echo 'FastAPI project created successfully!'",
    "echo 'Run with: uvicorn app.main:app --reload --host 0.0.0.0 --port 8000'"
  ],
  "tags": ["fastapi", "python", "api", "backend", "docker"],
  "preview": "FastAPI with automatic OpenAPI docs, JWT auth, and Docker deployment"
}